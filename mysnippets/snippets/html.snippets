	<br />
snippet button
	<button type="button">${1}</button>
snippet inputtext
	<input type="text" name="${1}">
snippet inputpwd
	<input type="password" name="${1}">
snippet inputcheck
	<input type="checkbox" name="${1}">
snippet inputradio
	<input type="radio" checked="${3:checked}" name="${1}" value="${2}">
snippet select
	<select>
		<option value="${1}">${2}</option>
		<option value="${3}" selected="selected">${4}</option>
	</select>
snippet textarea
	<textarea rows="${1}" cols="${2}">
