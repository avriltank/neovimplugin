snippet hr
	<hr/>
snippet form
	<form action="${1}" method="${2}">
		${3}
	</form>
snippet frameset
	<frameset cols="${1}%,${2}%${3}%">
		<frame src="${4}" />
		<frame src="${5}" />
		<frame src="${6}" />
	</frameset>
snippet iframe
	<iframe src="${1}">${2}</iframe>
snippet map
	<map name="${1}" id="${2}">
		<area shape="circle" coords="180,139,14" href ="" alt="" />
		<area shape="circle" coords="129,161,10" href ="" alt="" />
		<area shape="rect" coords="0,0,110,260" href ="" alt="" />
	</map>
snippet area
	<area shape="${1}" coords="${2}" href="${3}" alt="${4}"/>
snippet b
	<b>${1}</b>
snippet p
	<p>
		${1}
	</p>
snippet h1
	<h1>${1}</h1>
